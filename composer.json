{
  "name": "gispatial/CSVParse2Justed",
  "description": "PHP Multiformat Streaming Parser by JustEd aka R. Aidy",
  "keywords": ["php7", "laravel", "stream", "parser", "pull", "xml", "csv", "json"],
  "license": "MIT",

  "authors": [
    {
      "name": "JustEd",
      "homepage": "http://gispatial.tech/",
      "role": "frontend_software_engineer"
    }
  ],

  "require": {
    "php": "^7.1.3",
    "ext-xmlreader": "*",
    "maxakawizard/json-collection-parser": "^1.1",
    "tightenco/collect": "^5.7"
  },

  "require-dev": {
    "mockery/mockery": "^0.9.9",
    "phpunit/phpunit": "^6.0"
  },

  "autoload": {
    "psr-4": {
      "gispatial\\JustedCSVParser\\": "src"
    }
  },

  "autoload-dev": {
    "psr-4": {
      "gispatial\\JustedCSVParser\\Test\\": "tests"
    }
  },

  "scripts": {
    "test": "vendor/bin/phpunit"
  },

  "config": {
    "sort-packages": true
  }
}
